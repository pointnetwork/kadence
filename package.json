    "split": "1.0.1",
    "tiny": "0.0.10",
    "uuid": "3.2.1"
  },
  "devDependencies": {
    "chai": "^2.2.0",
    "coveralls": "^2.11.2",
    "eslint": "^3.14.0",
    "ink-docstrap": "git+https://gitlab.com/kadence/docstrap.git",
    "istanbul": "^1.1.0-alpha.1",
    "jsdoc": "^3.4.0",
    "memdown": "^2.0.0",
    "mocha": "^2.2.4",
    "proxyquire": "^1.8.0",
    "rimraf": "^2.6.1",
    "sinon": "^2.4.1"
  }
}
